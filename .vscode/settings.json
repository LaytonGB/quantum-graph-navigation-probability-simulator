{
  "rust-analyzer.rustfmt.extraArgs": ["+nightly"],
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.showUnlinkedFileNotification": false,
  "cSpell.words": [
    "consts",
    "Eframe",
    "egui",
    "evalexpr",
    "Hadamard",
    "nalgebra",
    "ncols",
    "nrows",
    "powi",
    "Repr"
  ],
  "rust-analyzer.cargo.features": [
    "auto-fill-qft"
  ]
}
